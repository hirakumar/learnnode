<h1>Employee</h1>
{{> adminnav}}
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Designation</th>
      <th scope="col">Department</th>
    </tr>
  </thead>
  <tbody>
      {{#each users}}
        <tr>
      <th scope="row">{{id}}</th>
      <td>{{username}}</td>
      <td>{{designation}}</td>
      <td>{{department}}</td>
    </tr>
      {{/each}}
  
   
  </tbody>
</table>